{% extends 'base.html.twig' %}

{% block title %}Hello AccueilController!{% endblock %}

{% block body %}



    <div class="col-12">
        <div class="row">
            <div class="col-12">
                <div class="jumbotron">
                    <div class="text text-center">
                        Bienvenue sur la boutique 404.
                    </div>
                </div>

            </div>
        </div>

        <div class="row">

            {% for i in 1..5 %}
                <div class="col-1">
                    <img src="{{ asset('images/gommettes'~i~'.jpg') }}"
                         class="rounded img-fluid " alt="gommettes rondes">
                </div>
            {% endfor %}
        </div>
    </div>

{% endblock %}
